{
  "name": "frontend_interview_test",
  "version": "1.0.0",
  "description":
    "An interview test for Front-End Development job candidates",
  "main": "index.html",
  "scripts": {
    "dev":
      "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile --open",
    "hotDebug":
      "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile",
    "debug":
      "webpack-dev-server --host localhost --port 8080 --env=dev --profile",
    "net2":
      "webpack-dev-server --host 192.168.1.227 --port 80 --env=dev --profile --open",
    "watch": "webpack --progress --colors --env=dev --profile --watch",
    "exportp":
      "node ./rmDir.js && webpack --env=prod --profile --progress --colors",
    "rm": "node ./rmDir.js",
    "clean": "node ./clean.js && npm i -D && npm up -D"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/frontend-code-challenge.git"
  },
  "author": "Carlos Velasco",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/frontend-code-challenge/issues"
  },
  "homepage":
    "https://github.com/xtreemze/frontend-code-challenge#readme",
  "browserslist": ["> 1%", "last 2 versions"],
  "dependencies": {
    "reload-html-webpack-plugin": "^0.1.2"
  },
  "devDependencies": {
    "@types/materialize-css": "^1.0.0",
    "autoprefixer": "^7.2.3",
    "browserslist": "^2.10.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "cssnano-preset-advanced": "^4.0.0-rc.2",
    "eslint": "^4.13.1",
    "extract-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-minifier-webpack-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "material-design-icons": "^3.0.1",
    "materialize-css": "1.0.0-alpha.1",
    "materialize-loader": "^2.0.2",
    "node-sass": "^4.7.2",
    "offline-plugin": "^4.8.5",
